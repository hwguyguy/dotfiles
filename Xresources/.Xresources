! font settings --------------------------------------------------------------

Xft.autohint: true
Xft.antialias: true
Xft.hinting: true
Xft.hintstyle: hintslight
!Xft.dpi: 96
Xft.rgba: rgb
Xft.lcdfilter: lcddefault

! urxvt ----------------------------------------------------------------------

! black
URxvt.color0  : #000000
URxvt.color8  : #555753
! red
URxvt.color1  : #cc0000
URxvt.color9  : #ef2929
! green
URxvt.color2  : #4e9a06
URxvt.color10 : #8ae234
! yellow
URxvt.color3  : #c4a000
URxvt.color11 : #fce94f
! blue
URxvt.color4  : #3465a4
URxvt.color12 : #729fcf
! magenta
URxvt.color5  : #75507b
URxvt.color13 : #ad7fa8
! cyan
URxvt.color6  : #06989a
URxvt.color14 : #34e2e2
! white
URxvt.color7  : #d3d7cf
URxvt.color15 : #eeeeec

URxvt.foreground:#d8d8d8
URxvt.background:#2e3436

URxvt.font: xft:Ubuntu Mono:pixelsize=16
URxvt.letterSpace: -1

URxvt.scrollBar: False
URxvt.scrollTtyOutput: False
URxvt.scrollWithBuffer: True
URxvt.scrollTtyKeypress: True
URxvt.secondaryScroll: True
URxvt.saveLines: 8000
URxvt.termName: xterm-256color

URxvt.geometry: 136x40
